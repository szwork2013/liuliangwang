
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
          提现申请
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                  <th>用户</th>
                  <th>微信</th>
                  <th>帐号</th>
                  <th>提现金额</th>
                  <th>手机号码</th>
                  <th>状态</th>
                  <th></th>
              </tr>
            </thead>
            <tbody>
              {{#each withdrawals.rows }}
                <tr>
                    <td>{{ name }}</td>
                    <td>{{ customer.username }}</td>
                    <td>{{ account }}</td>
                    <td>{{ amount }}</td>
                    <td>{{ phone }}</td>
                    <td>{{ withdrawalStatus state ../STATUS }}</td>
                    <td>
                    <a href="/admin/withdrawals/{{id}}/edit" class="list-group-item">
                      <i class="glyphicon glyphicon-pencil"></i>
                    </a>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        <!-- /.table-responsive -->

      </div>

      <div class="panel-footer">
          {{pagination withdrawals '/admin/withdrawals'}}
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>
</div>