<div class="space-1"></div>

<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <div class="well well-sm">
        <div class="row">
          <div class="col-xs-4">
            <img src="{{ one.headimgurl }}" onError="this.src='http://placehold.it/380x380';" class="img-rounded img-responsive">
          </div>
          <div class="col-xs-8">
            <h5>{{ one.username }}</h5>
            <p><small>关注时间： {{ strftime one.subscribeTime "YYYY-MM-DD" }}</small></p>
            <p><small>推荐人： {{ customer.username }} </small></p>
          </div>
        </div>
      </div>
    </div>

    <div class="col-xs-11 slave-detail">
      <div class="row">
        <div class="col-xs-6">
          <span>累计消费： ￥{{ one.orderTotal }}</span>
        </div>
        <div class="col-xs-6">
          <span>分销收入： ￥{{ one.salary }}</span>
        </div>
      </div>
    </div>

  <div class="container-fluid">
    <div class="row">
      {{#each result}}
      <div class="col-xs-12">
        <div class="panel-pl">
          <a class="coll-link" data-toggle="collapse" data-parent="#accordion" href="#collapse{{ @index }}">
            <div class="panel-heading">
              <h4 class="panel-title">
                  <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>    {{ name }}
                  <span class="badge pull-right orange-bg">{{ count }}</span>
              </h4>
            </div>
          </a>
          <div id="collapse{{ @index }}" class="panel-collapse collapse">
            <div class="panel-body">
              <ul class="slaves">
                <li>
                  <a href="#">
                    <span>
                      <i class="glyphicon glyphicon-user" ></i>   人数
                    </span>
                    <span class="badge pull-right orange-bg">{{ count }}</span>
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>


  </div>
</div>