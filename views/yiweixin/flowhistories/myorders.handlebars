<div class="container dtb_distribution">
  {{> yiweixin/myaccount-header  }}

   {{#if extractOrders }}
      <div class="container-fluid" style="padding-top: 10px;">
        {{#each extractOrders}}
          <div class="row">
            <div class="col-lg-12 col-xs-12">
              <div class="panel {{ orderStyle this }}">
                <div class="panel-heading">
                  <small class="pull-right text-muted">
                     <i class="glyphicon glyphicon-dashboard"></i> {{ strftime createdAt 'YYYY-MM-DD HH:mm' }}
                  </small>
                  我的订单
                </div>
                <div class="panel-body">
                    <p>
                      向{{ phone }}充值{{ trafficPlan.name }}
                    </p>
                    <p>
                      花费: ￥{{ total }}
                    </p>
                    <p>
                      状态: {{ stateName }}
                    </p>
                </div>
                <div class="panel-footer">
                </div>
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    {{else}}
      {{> no_data_found }}
    {{/if}}

 </div>

